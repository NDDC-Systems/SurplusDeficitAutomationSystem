@model IEnumerable<SurplusDeficitAutomationSystem.Models.StringField>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StringValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Field)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StringValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Field.FieldId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.StringFieldId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.StringFieldId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.StringFieldId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
